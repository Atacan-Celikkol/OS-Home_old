{
   "manifest_version": 2,
   "name": "OttoLauncher",
   "version": "1.2",
   "description": "Home sweet home.",
   "browser_action": {
      "default_icon": "OttoStudio.png",
      "default_popup": "index.html"
   },
   "icons": {
      "128": "OttoStudio.png"
   },
   "permissions": [
      "management",
      "history"
   ],
   "web_accessible_resources": [
      "assets/css/*",
      "assets/js/*",
      "assets/fonts/*"
   ],
   "chrome_url_overrides": {
      "newtab": "index.html"
   }
}